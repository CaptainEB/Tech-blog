<h1>Tech Blog</h1>
<ul class="posts-ul">
	{{#if posts}}
		{{#each posts as |post|}}
			<li class="posts-li">
				<div class='post-header'>
					<h2>{{post.title}}</h2>
					<p>{{post.created_at}}</p>
					<p>{{post.user_id}}</p>
				</div>
				<p>{{post.content}}</p>
			</li>
		{{/each}}
	{{/if}}
</ul>